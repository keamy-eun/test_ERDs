
CREATE TABLE 리뷰들
(
  UNIQUE_ID_USERS VARCHAR(200) NOT NULL COMMENT '사용자들대표값',
  REVIEW          VARCHAR(200) NULL     COMMENT '리뷰제목',
  COMMENT         VARCHAR(200) NULL     COMMENT '내용',
  CREATED_DATE    INTEGER(200) NULL     COMMENT '작성일',
  UNIQUE_ID_NAME  VARCHAR(200) NOT NULL COMMENT '영화이름대표값',
  PRIMARY KEY (UNIQUE_ID_USERS)
);

CREATE TABLE 사용자들
(
  USERS           VARCHAR(200) NULL     COMMENT '사용자들',
  UNIQUE_ID_USERS VARCHAR(200) NOT NULL COMMENT '사용자들대표값',
  PRIMARY KEY (UNIQUE_ID_USERS)
);

CREATE TABLE 영화이름들
(
  UNIQUE_ID_NAME   VARCHAR(200) NOT NULL COMMENT '영화이름대표값',
  UNIQUE_ID_REIVEW VARCHAR(200) NOT NULL COMMENT '리뷰대표값'
);

CREATE TABLE 영화이름목록
(
  NAME           VARCHAR(200) NULL     COMMENT '영화이름',
  UNIQUE_ID_NAME VARCHAR(200) NOT NULL COMMENT '영화이름대표값',
  PRIMARY KEY (UNIQUE_ID_NAME)
);

CREATE TABLE 작성자들
(
  UNIQUE_ID_USERS  VARCHAR(200) NOT NULL COMMENT '사용자들대표값',
  UNIQUE_ID_REIVEW VARCHAR(200) NOT NULL COMMENT '리뷰대표값'
);

ALTER TABLE 영화이름들
  ADD CONSTRAINT FK_영화이름목록_TO_영화이름들
    FOREIGN KEY (UNIQUE_ID_NAME)
    REFERENCES 영화이름목록 (UNIQUE_ID_NAME);

ALTER TABLE 영화이름들
  ADD CONSTRAINT FK_리뷰들_TO_영화이름들
    FOREIGN KEY (UNIQUE_ID_REIVEW)
    REFERENCES 리뷰들 (UNIQUE_ID_USERS);

ALTER TABLE 작성자들
  ADD CONSTRAINT FK_사용자들_TO_작성자들
    FOREIGN KEY (UNIQUE_ID_USERS)
    REFERENCES 사용자들 (UNIQUE_ID_USERS);

ALTER TABLE 작성자들
  ADD CONSTRAINT FK_리뷰들_TO_작성자들
    FOREIGN KEY (UNIQUE_ID_REIVEW)
    REFERENCES 리뷰들 (UNIQUE_ID_USERS);
